{
  "name": "@pbcomponents/vue",
  "version": "0.0.1",
  "description": "UI component library for Vue with Typescript and Tailwind",
  "repository": {
    "type": "git",
    "url": "https://github.com/prosazhin/vue-pbcomponents.git"
  },
  "author": "Evgenii Sazhin <prosazhin@gmail.com> (https://prosazhin.ru)",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vue-tsc && vite build",
    "lint": "vue-tsc --noEmit && eslint src",
    "lint:fix": "eslint --fix src --ext .ts,.vue",
    "lint:markup": "vue-tsc --noEmit",
    "clean": "rimraf ./dist"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.1.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.8",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-vue": "^9.27.0",
    "pbstyles": "^1.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.2",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^4.0.3",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.29"
  },
  "keywords": [
    "vue",
    "typescript",
    "tailwindcss",
    "ui",
    "components"
  ],
  "contributors": [
    {
      "name": "Katya Lebedeva",
      "email": "le.ktrna@gmail.com",
      "url": "https://github.com/klebedeva"
    }
  ]
}
